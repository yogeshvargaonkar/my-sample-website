{
    "containerDefinitions": [
        {
            "name": "SampleWebsite",
            "image": "339712877997.dkr.ecr.us-east-1.amazonaws.com/my-sample-website/my-sample-website:1.0.18",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "name": "samplewebsite-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "family": "my-sample-website",
    "executionRoleArn": "arn:aws:iam::339712877997:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "compatibilities": [
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": 256,
    "memory": 512,
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
}