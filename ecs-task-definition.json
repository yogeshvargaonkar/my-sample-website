{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "my-sample-website",
    "containerDefinitions": [
        {
            "name": "SampleWebsite",
            "image": "339712877997.dkr.ecr.us-east-1.amazonaws.com/my-sample-website/my-sample-website:1.0.18",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::339712877997:role/ecsTaskExecutionRole"
}
